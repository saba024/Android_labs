<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".KeyboardFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:rowCount="3"
    android:columnCount="3">

    <Button android:id="@+id/button1" style="@style/KeyboardButtonLand" android:text="1"/>
    <Button android:id="@+id/button2" style="@style/KeyboardButtonLand" android:text="2"/>
    <Button android:id="@+id/button3" style="@style/KeyboardButtonLand" android:text="3"/>
    <Button android:id="@+id/button4" style="@style/KeyboardButtonLand" android:text="4"/>
    <Button android:id="@+id/button5" style="@style/KeyboardButtonLand" android:text="5"/>
    <Button android:id="@+id/button6" style="@style/KeyboardButtonLand" android:text="6"/>
    <Button android:id="@+id/button7" style="@style/KeyboardButtonLand" android:text="7"/>
    <Button android:id="@+id/button8" style="@style/KeyboardButtonLand" android:text="8"/>
    <Button android:id="@+id/button9" style="@style/KeyboardButtonLand" android:text="9"/>
    <Button android:id="@+id/buttonDot" style="@style/KeyboardButtonLand" android:text="."/>
    <Button android:id="@+id/button0" style="@style/KeyboardButtonLand" android:text="0"/>
    <Button android:id="@+id/buttonDel" style="@style/KeyboardButtonLand" android:text="C"/>
</GridLayout>